{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "SwaggerDisabled": true
  },
  "OpenTelemetry": {
    "Endpoint": null
  },
  "Cors": {
    "Origins": [
    ]
  },
  "Authentication": {
    "Authority": "---",
    "ClientId": "---",
    "ClientSecret": "---"
  },
  "ReverseProxy": {
    "Routes": {
      "FileService-health": {
        "ClusterId": "FileService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/FileService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      },
      "FileService": {
        "ClusterId": "FileService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/FileService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      },
      "HashService-health": {
        "ClusterId": "HashService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/HashService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/HashService-api"
          }
        ]
      },
      "HashService": {
        "ClusterId": "HashService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/HashService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/HashService-api"
          }
        ]
      }
    },
    "Clusters": {
      "FileService-api": {
        "Destinations": {
          "primary": {
            "Address": "http://file_api"
          }
        }
      },
      "HashService-api": {
        "Destinations": {
          "primary": {
            "Address": "http://hash_api"
          }
        }
      }
    }
  },
  "HealthChecksUI": {
    "DisableMigrations": true,
    "HealthChecks": [
      {
        "Name": "self",
        "Uri": "http://127.0.0.1/healthChecks"
      },
      {
        "Name": "Wally.RomMaster.FileService",
        "Uri": "http://127.0.0.1/FileService-api/healthChecks"
      },
      {
        "Name": "Wally.RomMaster.HashService",
        "Uri": "http://127.0.0.1/HashService-api/healthChecks"
      }
    ],
    "Webhooks": [
//      {
//        "Name": "",
//        "Uri": "",
//        "Payload": "",
//        "RestoredPayload": ""
//      }
    ],
    "EvaluationTimeInSeconds": 600,
    "MinimumSecondsBetweenFailureNotifications": 600,
    "MaximumExecutionHistoriesPerEndpoint": 100,
    "HealthCheckDatabaseConnectionString": "Data Source=healthChecks"
  }
}
