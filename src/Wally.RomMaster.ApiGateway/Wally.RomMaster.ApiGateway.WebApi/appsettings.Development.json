{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "SwaggerDisabled": false
  },
  "OpenTelemetry": {
    "Endpoint": "http://aspire:18888"
  },
  "Cors": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:8080"
    ]
  },
  "Authentication": {
    "Authority": "https://localhost:44310",
    "ClientId": "Wally.RomMaster_api",
    "ClientSecret": "---"
  },
  "SwaggerAuthentication": {
    "Authority": "https://localhost:44310",
    "ClientId": "Wally.RomMaster_swagger",
    "ClientSecret": "---"
  },
  "ReverseProxy": {
    "Routes": {
      "FileService-health": {
        "ClusterId": "FileService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/FileService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      },
      "FileService": {
        "ClusterId": "FileService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/FileService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      },
      "HashService-health": {
        "ClusterId": "HashService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/HashService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/HashService-api"
          }
        ]
      },
      "HashService": {
        "ClusterId": "HashService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/HashService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/HashService-api"
          }
        ]
      }
    },
    "Clusters": {
      "FileService-api": {
        "Destinations": {
          "primary": {
            "Address": "https://localhost:7181"
          }
        }
      },
      "HashService-api": {
        "Destinations": {
          "primary": {
            "Address": "https://localhost:7182"
          }
        }
      }
    }
  }
}
