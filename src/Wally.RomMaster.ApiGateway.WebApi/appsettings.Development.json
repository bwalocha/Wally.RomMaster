{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:8080"
    ]
  },
  "Authentication": {
    "Authority": "https://localhost:44310",
    "ClientId": "Wally.RomMaster_api",
    "ClientSecret": "---"
  },
  "ReverseProxy": {
    "Routes": {
      "FileService-health": {
        "ClusterId": "FileService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/FileService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      },
      "FileService": {
        "ClusterId": "FileService-api",
        "Match": {
          "Path": "/FileService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/FileService-api"
          }
        ]
      }
    },
    "Clusters": {
      "FileService-api": {
        "Destinations": {
          "primary": {
            "Address": "https://localhost:7181"
          }
        }
      }
    }
  }
}
