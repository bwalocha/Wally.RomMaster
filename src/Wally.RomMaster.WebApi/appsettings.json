{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "DisableMigrations": true,
    "HealthChecks": [
      {
        "Name": "self",
        "Uri": "/healthChecks"
      }
    ],
    "Webhooks": [
      {
        "Name": "sampleWebHook",
        "Uri": "",
        "Payload": "",
        "RestoredPayload": ""
      }
    ],
    "EvaluationTimeInSeconds": 600,
    "MinimumSecondsBetweenFailureNotifications": 600,
    "MaximumExecutionHistoriesPerEndpoint": 100,
    "HealthCheckDatabaseConnectionString": "Data Source=---"
  },
  "Cors": {
    "Origins": [
      "---"
    ]
  },
  "Authentication": {
    "Authority": "---",
    "ClientId": "Wally.RomMaster_API",
    "ClientSecret": "---"
  },
  "ConnectionStrings": {
    "Database": "Data Source=---;Database=Wally.RomMaster;Persist Security Info=True;User ID=---;Password=---;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False"
  },
  "DatRoots": [
    {
      "Enabled": false,
      "Path": "---",
      "SearchOptions": "AllDirectories",
      "WatcherEnabled": "true",
      "Excludes": [
        {
          "Pattern": "*.txt"
        }
      ]
    }
  ],
  "RomRoots": [
    {
      "Enabled": false,
      "Path": "---",
      "SearchOptions": "AllDirectories",
      "WatcherEnabled": "true",
      "Excludes": [
        {
          "Pattern": "__RomVault.tmp"
        },
        {
          "Pattern": "*.tmp"
        }
      ]
    }
  ],
  "ToSortRoots": [
    {
      "Enabled": false,
      "Path": "---",
      "SearchOptions": "AllDirectories",
      "WatcherEnabled": "true"
    }
  ]
}
